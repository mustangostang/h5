    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-actions": "^0.13.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "sass-loader": "^3.1.1",
    "string-format": "^0.5.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.16.2"
  },
